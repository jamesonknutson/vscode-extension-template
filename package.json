{
  "activationEvents": [
    "onCommand:vscode-extension-template.helloWorld"
  ],
  "categories": [
    "Other"
  ],
  "contributes": {
    "commands": [
      {
        "command": "vscode-extension-template.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "displayName": "vscode-extension-template",
  "engines": {
    "vscode": "^1.79.0",
    "npm": "^9.5.0",
    "pnpm": "please-use-npm",
    "yarn": "please-use-npm"
  },
  "main": "./out/extension.js",
  "name": "vscode-extension-template",
  "scripts": {
    "test-compile": "tsc -p ./",
    "compile": "tsc -p ./",
    "esbuild": "rimraf ./out && npm run esbuild-base -- --sourcemap",
    "esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=out/extension.js --external:vscode --format=cjs --platform=node --log-level=info",
    "esbuild-watch": "npm run esbuild-base -- --sourcemap --watch",
    "esbuild-dev": "rimraf ./out && npm run esbuild-base -- --sourcemap",
    "vscode:prepublish": "rimraf ./out && npm run esbuild-base -- --minify",
    "watch": "tsc -watch -p ./",
    "generate-schema": "typescript-json-schema ./src/configuration.ts TConfiguration --refs false --aliasRefs false --topRef false --required true --strictNullChecks true --out ./configuration-json-schema.json",
    "generate-vscode-types": "typed-vscode generate --out src/generated.ts",
    "package": "vsce package --out ./packages"
  },
  "version": "0.0.1",
  "author": "jamesonknutson",
  "description": "Template Repository for new VSCode Extension Projects",
  "devDependencies": {
    "@jamesonknutson/eslint-config": "^1.0.2",
    "@jamesonknutson/tsconfig": "^1.0.1",
    "@types/node": "~16.18.35",
    "@types/vscode": "^1.79.0",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "esbuild": "^0.18.17",
    "eslint": "^8.46.0",
    "prettier": "^2.8.8",
    "prettier-plugin-jsdoc": "^0.4.2",
    "rimraf": "^5.0.1",
    "ts-json-schema-generator": "^1.2.0",
    "tslib": "^2.6.1",
    "type-fest": "^4.0.0",
    "typed-vscode": "^0.0.5",
    "typescript": "^5.1.6",
    "typescript-json-schema": "^0.59.0"
  },
  "keywords": [],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/jamesonknutson/vscode-extension-template.git"
  }
}
