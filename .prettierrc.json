{
  "parser": "typescript",
  "printWidth": 80,
  "semi": false,
  "singleQuote": true,
  "arrowParens": "avoid",
  "bracketSpacing": true,
  "eslintIntegration": true,
  "endOfLine": "lf",
  "useTabs": false,
  "tabWidth": 2,
  "trailingComma": "es5",
  "quoteProps": "consistent",
  "jsxSingleQuote": true,
  "plugins": ["./node_modules/prettier-plugin-jsdoc/dist/index.js"],
  "jsdocDescriptionTag": false,
  "jsdocPreferCodeFences": true,
  "jsdocPrintWidth": 100
}